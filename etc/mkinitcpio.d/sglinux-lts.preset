# mkinitcpio preset file for the 'linux' package

ALL_config="/etc/mkinitcpio.conf.sg"
ALL_kver="/boot/vmlinuz-linux-lts"

PRESETS=('default' 'fallback' 'livecd' 'livecd_lite')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/sg-initramfs-linux-lts.img"
default_uki="/boot/EFI/sglinux/sglinux-lts.efi"
default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/sg-initramfs-linux-lts-fallback.img"
fallback_uki="/boot/EFI/sglinux/sglinux-lts-fallback.efi"
fallback_options="-S autodetect"

#livecd_config="/etc/mkinitcpio.conf"
livecd_image="/boot/sg-initramfs-linux-lts-livecd.img"
livecd_uki="/boot/EFI/sglinux/sglinux-lts-livecd.efi"
livecd_options="-S autodetect --cmdline /etc/kernel/livecd-cmdline.conf"

#livecd_lite_config="/etc/mkinitcpio.conf"
livecd_lite_image="/boot/sg-initramfs-linux-lts-livecd-lite.img"
livecd_lite_uki="/boot/EFI/sglinux/sglinux-lts-livecd-lite.efi"
livecd_lite_options="--cmdline /etc/kernel/livecd-cmdline.conf"
